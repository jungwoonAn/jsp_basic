<script type="text/javascript">
	function validateForm(form){
		if(!form.user_id.value){
			alert("아이디를 입력하세요.");
			return false;
		}
		if(form.user_pass.value==""){
			alert("패스워드를 입력하세요");
			return false;
		}
	}
</script>
